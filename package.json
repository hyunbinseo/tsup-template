{
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsup --format cjs,esm --dts --clean && publint",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "publint": "^0.2.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
